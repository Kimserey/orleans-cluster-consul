{
  "Serilog": {
    "IsEnabled": true,
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "Orleans": {
    "Cluster": {
      "Globals": {
        "DeploymentId": "App1",
      },
      "Defaults": {
        "HostNameOrIPAddress": "localhost",
        "Port": 30007,
        "ProxyGatewayEndpoint": {
          "Address": "localhost",
          "Port": 40007
        }
      }
    }
  }
}