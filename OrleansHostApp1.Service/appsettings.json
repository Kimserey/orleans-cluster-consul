{
  "Serilog": {
    "IsEnabled": true,
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Orleans": {
    "Cluster": {
      "Globals": {
        "DeploymentId": "App1",
      },
      "Defaults": {
        "HostNameOrIPAddress": "localhost",
        "Port": 30005,
        "ProxyGatewayEndpoint": {
          "Address": "localhost",
          "Port": 40005
        }
      }
    }
  }
}